public class Aluno 
{
	private String nome;
	private int celular;
	
	Aluno()
	{
		System.out.println("Bem vindo a vida estudantil");
	}
	
	public String getNome() 
	{
		return nome;
	}
	public void setNome(String nome)
	{
		this.nome = nome;
	}

	public int getCelular() {
		return celular;
	}

	public void setCelular(int celular) {
		this.celular = celular;
	}
	

}
